{
  "name": "my-react-router",
  "version": "0.0.4",
  "type": "module",
  "main": "lib/index.js",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "types": "lib/types/src/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && npm run prepare",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "pre:prepare": "npm run clean",
    "prepare": "swc src -d lib && npm run clean:lib && cp package.json lib && cp README.md lib && tsc --project tsconfig.types.json",
    "clean": "rm -rf lib",
    "clean:lib": "rm lib/App.js && rm lib/Router.test.js && rm lib/main.js && rm lib/vite-env.d.js && rm -rf lib/pages",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "path-to-regexp": "^6.2.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.64",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "@vitest/ui": "^0.31.4",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "happy-dom": "^9.20.3",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vitest": "^0.31.4"
  }
}
