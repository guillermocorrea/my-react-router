{
    "$schema": "https://json.schemastore.org/swcrc",
    "jsc": {
    "parser": {
        "syntax": "typescript",
        "tsx": true,
        "dynamicImport": false,
        "decorators": false
    },
    "transform": {
        "react": {
            "runtime": "automatic"
        }
    },
    "target": "es2020",
    "loose": true,
    "externalHelpers": false,
    "keepClassNames": false
    },
    "minify": true
   }
   